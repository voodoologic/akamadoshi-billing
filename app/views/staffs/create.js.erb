<%unless @staff_exists%>
<% inlineform_action = params[:add_more] ? "inlineform:save_and_add_more" : "inlineform:save" %>
<%= "{\"action\": \"#{inlineform_action}\",\"exists\": \"#{@staff_exists}\", \"record\": \"<option value='#{@staff.id}' selected type='#{params[:association] == "account" ? "account_level" : "company_level"}'>#{@staff.name}</option>\"}".html_safe%>
<%else%>
<%= "{\"exists\": \"#{@staff_exists}\"}".html_safe%>
<%end%>