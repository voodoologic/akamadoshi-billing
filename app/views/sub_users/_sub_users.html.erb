<% if  @sub_users.length > 0 %>
    <% @sub_users.each do |sub_user| %>
        <tr>
          <td class="align_left text-overflow-class"><%= sub_user.user_name %></td>
          <td class="align_left text-overflow-class"><%= sub_user.email %></td>
          <td class="align_left text-overflow-class"><%= sub_user.roles.first.try(:name).try(:humanize) %></td>
          <td class="align_left" ><%= sub_user.created_at.to_time.strftime(get_date_format) %></td>
          <td class="align_center"><%= link_to "Edit", edit_sub_user_path(sub_user) %></td>
          <!--<td class="align_center"><%#= link_to "Delete", destroy_sub_user_path(sub_user), remote: true %></td>-->
        </tr>
    <% end %>

<% else %>
    <tr>
      <td colspan="8">
        <div class="no_record_container">No users found</div>
      </td>
    </tr>
<% end %>