$('body').append("<%= escape_javascript render partial: 'unpaid_invoices' %>");

Payment.load_functions();
elem = $("#unpaid_invoices");
Popup.initialize(elem);
Popup.open(elem);

jQuery('#invoice_selection').submit(function() {
    var flag, invoices;
    invoices = $('#unpaid_invoices').find('tbody');
    var errorDiv = $('#unpaid_invoices').find('table');
    return flag = invoices.find('tr.no-invoices').length ? (applyPopover(errorDiv,"topMiddle","topLeft",I18n.t("views.payments.no_unpaid_invoice")), false) : invoices.find(":checked").length === 0 ? (applyPopover(errorDiv,"topMiddle","topLeft",I18n.t("views.payments.no_invoice_selected")), false) : true;
});
